<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>dotnet_result</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.0.2</Version>
    <PackageProjectUrl>https://github.com/TransientError/dotnet-result</PackageProjectUrl>
    <RepositoryUrl>https://github.com/TransientError/dotnet-result</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Kevin Wu</Authors>
    <Description>A lightweight and type-safe result type for .NET.</Description>
    <Summary>A lightweight and type-safe result type for .NET.</Summary>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>
</Project>
